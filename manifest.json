{
    "manifest_version": 3,
    "name": "EmailX - Email Scraper",
    "description": "This extension extract emails from the current webpage.",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/emailx.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}
